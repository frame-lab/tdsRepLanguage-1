modoEntrada = false // pode ser um parâmetro passado pro programa, por exemplo

tdsA = {
	portname : 'A'
	data-time: {0: 0, 1: 1, 2: 0} 
	link: tdsC
} 
	
tdsB = {
	portname : 'B'
	data-time: { function-domain: fluxoPortempo(conjuntoDedados,condicoes,...) } // nesse caso é definido por 
	link: tdsC
}


function fluxoPortempo(tempo , modoEntrada){
	

	if(modoEntrada == true and tempo != 0) {
		dado = t % 2 
	}
	if(modoEntrada == false and tempo != 0) {
		dado = t % 2 + 1	

	}

}

tdsC = {
	
	linked: {tdsA, tdsB} 
}



